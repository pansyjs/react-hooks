{
  "name": "@pansy/react-hooks",
  "version": "0.0.2",
  "main": "cjs/index.js",
  "esnext": "esnext/index.js",
  "types": "cjs/index.d.ts",
  "module": "esm/index.js",
  "keywords": [
    "react",
    "hook",
    "react-hook",
    "browser",
    "ssr"
  ],
  "repository": "git@github.com:pansyjs/react-hooks.git",
  "author": "wangxingkang <wang_xingkang@qq.com>",
  "license": "MIT",
  "files": [
    "cjs",
    "esm",
    "esnext"
  ],
  "scripts": {
    "start": "dumi dev",
    "site": "dumi build",
    "build": "npm run entry && npm run build:clean && npm run build:all",
    "build:all": "npm run build:cjs && npm run build:esm && npm run build:esnext",
    "build:clean": "rimraf ./cjs ./esm ./esnext",
    "build:cjs": "tsc -p ./tsconfig.build.json --module CommonJS --target ES5 --outDir ./cjs",
    "build:esm": "tsc -p ./tsconfig.build.json --module ES6 --target ES5 --outDir ./esm",
    "build:esnext": "tsc -p ./tsconfig.build.json --module ESNext --target ESNext --outDir ./esnext",
    "lint:es": "eslint ./src --ext '.ts,.tsx'",
    "lint:esfix": "yarn lint:es --fix",
    "commit": "commit",
    "entry": "walrus entry",
    "test": "walrus-test",
    "release": "release"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "7.0.1",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "@walrus/cli": "1.3.4",
    "@walrus/commit": "1.2.1",
    "@walrus/plugin-entry": "0.2.1",
    "@walrus/release-cli": "0.4.0",
    "@walrus/test": "1.3.0",
    "antd": "4.16.5",
    "dumi": "1.1.22",
    "eslint": "7.29.0",
    "eslint-config-alloy": "4.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
